  "scripts": {
    "install:deps": "node install.js",
    "start": "node server.js",
    "lint": "eslint . --fix --cache & pnpm exec stylelint \"**/*.css\" --fix",
    "lint:check": "eslint . --cache & pnpm exec stylelint \"**/*.{css,html,js,jsx,ts,tsx}\"",
    "lint:scripts": "eslint . --fix --cache",
    "lint:styles": "stylelint \"**/*.{css,html,js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "node build.js",
    "build:debug": "node build.js --env=debug",
    "test:unit": "cross-env NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:CI": "pnpm test:unit & pnpm test:e2e",
    "postinstall": "playwright install",
    "prepare": "husky"
  },
